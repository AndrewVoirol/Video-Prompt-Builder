{
  "name": "video-prompt-builder",
  "version": "2.0.0",
  "private": true,
  "description": "A fully responsive, accessible Next.js and TypeScript video prompt builder for AI video models",
  "homepage": "https://github.com/AndrewVoirol/Video-Prompt-Builder",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewVoirol/Video-Prompt-Builder.git"
  },
  "bugs": {
    "url": "https://github.com/AndrewVoirol/Video-Prompt-Builder/issues"
  },
  "author": "AndrewVoirol",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript",
    "ai",
    "video",
    "prompt-builder",
    "tailwindcss",
    "react",
    "accessibility",
    "responsive"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next out dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "mcp:server": "ts-node scripts/puppeteer-mcp-server.ts",
    "mcp:build": "tsc scripts/puppeteer-mcp-server.ts --outDir dist/scripts --module node16 --moduleResolution node16",
    "mcp:test": "pnpm mcp:build && ts-node scripts/test-mcp-server.ts",
    "test:e2e": "ts-node -P tests/e2e/tsconfig.json tests/e2e/theme-test.runner.ts",
    "test:theme:runner": "ts-node tests/e2e/theme-test.runner.ts",
    "test:theme:example": "ts-node tests/e2e/example-theme-test.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.300.0",
    "next": "^15.4.5",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.61.1",
    "react-resizable-panels": "^3.0.4",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^2.0.6",
    "tw-animate-css": "^1.3.6",
    "vaul": "^1.1.2",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^24.1.0",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "babel-jest": "^30.0.5",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "cross-spawn": "^7.0.6",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^12.8.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "puppeteer": "^24.15.0",
    "puppeteer-mcp-server": "^0.7.2",
    "shadcn": "^2.10.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
