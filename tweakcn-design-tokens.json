  "typography": {
    "fontFamilies": {
      "sans": "Geist Mono, monospace",
      "serif": "Geist Mono, monospace", 
      "mono": "Geist Mono, monospace"
    },
    "letterSpacing": {
      "normal": "0em"
    },
    "tracking": {
      "tighter": "calc(var(--tracking-normal) - 0.05em)",
      "tight": "calc(var(--tracking-normal) - 0.025em)",
      "normal": "var(--tracking-normal)",
      "wide": "calc(var(--tracking-normal) + 0.025em)",
      "wider": "calc(var(--tracking-normal) + 0.05em)",
      "widest": "calc(var(--tracking-normal) + 0.1em)"
    }
  },
  "radius": {
    "base": "0rem",
    "sm": "calc(var(--radius) - 4px)",
    "md": "calc(var(--radius) - 2px)", 
    "lg": "var(--radius)",
    "xl": "calc(var(--radius) + 4px)"
  },
  "shadows": {
    "config": {
      "color": "hsl(0 0% 0%)",
      "opacity": "0",
      "blur": "0px",
      "spread": "0px",
      "offsetX": "0px",
      "offsetY": "1px"
    },
    "variants": {
      "2xs": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00)"
      },
      "xs": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00)"
      },
      "sm": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 1px 2px -1px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 1px 2px -1px hsl(0 0% 0% / 0.00)"
      },
      "default": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 1px 2px -1px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 1px 2px -1px hsl(0 0% 0% / 0.00)"
      },
      "md": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 2px 4px -1px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 2px 4px -1px hsl(0 0% 0% / 0.00)"
      },
      "lg": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 4px 6px -1px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 4px 6px -1px hsl(0 0% 0% / 0.00)"
      },
      "xl": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 8px 10px -1px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00), 0px 8px 10px -1px hsl(0 0% 0% / 0.00)"
      },
      "2xl": {
        "light": "0px 1px 0px 0px hsl(0 0% 0% / 0.00)",
        "dark": "0px 1px 0px 0px hsl(0 0% 0% / 0.00)"
      }
    }
  },
  "spacing": {
    "base": "0.25rem"
  },
  "motion": {
    "easing": {
      "default": "cubic-bezier(0.4, 0.36, 0, 1)",
      "inOut": "ease-in-out"
    },
    "duration": {
      "default": "300ms",
      "reveal": "400ms"
    }
  },
  "architecture": {
    "cssVariables": {
      "prefix": "--",
      "colorFormat": "oklch",
      "supportsAlpha": true
    },
    "themes": {
      "selector": {
        "light": ":root",
        "dark": ".dark"
      }
    }
  }
}
